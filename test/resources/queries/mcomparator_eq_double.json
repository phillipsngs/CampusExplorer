{
    "title": "EQ accepts a double",
    "input": {
        "WHERE": {
            "EQ": {
                "courses_avg": 99.78
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_id",
                "courses_avg"
            ],
            "ORDER": "courses_avg"
        }
    },
    "expected": [
        {
            "courses_id": "527",
            "courses_avg": 99.78
        },
        {
            "courses_id": "527",
            "courses_avg": 99.78
        }
    ]
}
